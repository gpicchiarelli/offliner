				<dict>
					<key>COMMAND_STRING</key>
					<string># OffLiner - Quick Action per Finder
# Scarica il sito web selezionato

# Estrai URL dal testo selezionato
URL=$(echo "$1" | grep -oE 'https?://[^[:space:]]+' | head -1)

if [ -z "$URL" ]; then
    # Se non c'Ã¨ un URL, usa il testo come URL
    URL="$1"
fi

# Verifica che sia un URL valido
if [[ ! "$URL" =~ ^https?:// ]]; then
    osascript -e 'display dialog "URL non valido: '"$URL"'" buttons {"OK"} default button "OK" with icon stop'
    exit 1
fi

# Esegui OffLiner
if command -v offliner &gt; /dev/null 2&gt;&amp;1; then
    # Apri Terminal e esegui offliner in background
    osascript -e 'tell application "Terminal" to do script "offliner --url '"$URL"' --output-dir \"$HOME/Downloads/OffLiner\" --verbose; osascript -e '\''display notification \"Download completato per '"$URL"'\" with title \"OffLiner\"'\''"'
    
    # Notifica immediata
    osascript -e 'display notification "Download avviato per '"$URL"'" with title "OffLiner"'
else
    osascript -e 'display dialog "OffLiner non trovato. Installalo con: ./macos/install.sh" buttons {"OK"} default button "OK" with icon stop'
fi</string>
